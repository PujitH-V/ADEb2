								},
								"sink": {
									"name": "CountryRegion",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "PostalCode",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "PostalCode",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "rowguid",
									"type": "Guid",
